{
  "name": "simple-bool",
  "version": "1.3.0",
  "description": "A simple set of functions that return a boolean.",
  "main": "dist/index.min.js",
  "files": [
    "dist/*"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "npm run build && npx jasmine",
    "start": "ts-node-dev --respawn src/index.ts",
    "build": "rmdir /s /Q dist & tsc && terser dist/index.js --output dist/index.min.js --config_file terserconfig.json --comments /^\\*\\n/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joker876/simple-bool.git"
  },
  "bugs": {
    "url": "https://github.com/joker876/simple-bool/issues"
  },
  "homepage": "https://github.com/joker876/simple-bool#readme",
  "author": "joker876",
  "license": "MIT",
  "devDependencies": {
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3",
    "@types/jasmine": "^4.0.3",
    "jasmine": "^4.1.0",
    "jasmine-ts-console-reporter": "^3.1.1"
  },
  "keywords": [
    "bool",
    "boolean",
    "is",
    "type",
    "typeof",
    "if",
    "yes",
    "no",
    "yesno",
    "evaluate"
  ]
}
